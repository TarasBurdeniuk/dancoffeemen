{
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					"jsx"
				]
			}
		],
		"prettier/prettier": ["error", {
			"endOfLine": "auto"
		}],
		"max-len": [
			"error",
			500
		]
	},
	"env": {
		"browser": true
	}
}
